{
  "expo": {
    "name": "Facto Mobile",
    "slug": "facto-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.facto.mobile",
      "scheme": "factomobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.facto.mobile",
      "scheme": "factomobile"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker"
    ],
    "platforms": ["ios", "android"],
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "scheme": "factomobile",
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "sdkVersion": "52.0.0"
  }
}